<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kuang.swagger.mapper.ProjectMapper">
<insert id="inserProject" parameterType="Project">
    insert into project (stu_id,pro_name,pro_user,pro_content,pro_skill) values (#{stu_id},#{pro_name},#{pro_user},#{pro_content},#{pro_skill})
</insert>
    <select id="queryProjectByid" resultType="Project">
        select * from project where stu_id=#{stu_id}
    </select>
</mapper>